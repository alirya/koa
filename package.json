{
  "name": "@dikac/tn-koa-router",
  "version": "0.8.10",
  "description": "Typescript koa router",
  "dependencies": {
    "@dikac/t-array": "^0.26.0-alpha.4",
    "@dikac/t-code": "^0.3.0-alpha.1",
    "@dikac/t-function": "^0.15.0-beta.9",
    "@dikac/t-http": "^0.18.0-alpha.3",
    "@dikac/t-message": "^0.12.0-alpha.2",
    "@dikac/t-number": "^0.17.0-alpha.4",
    "@dikac/t-object": "^0.37.0-alpha.5",
    "@dikac/t-string": "^0.16.0-alpha.4",
    "@dikac/t-syslog": "^0.2.3",
    "@dikac/t-validatable": "^0.20.0-beta.7",
    "@dikac/t-validator": "^0.19.0-beta.20",
    "@dikac/t-value": "^0.15.0-beta.2",
    "@koa/router": "^10.1.1",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "koa": "^2.13.4",
    "ts-toolbelt": "^9.6.0",
    "type-fest": "^2.3.3",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@dikac/tn-filesystem": "^0.4.2",
    "@types/jasmine": "^3.6.9",
    "@types/node": "^12.20.10",
    "axios": "=0.19.0",
    "cpy-cli": "^3.1.1",
    "esm": "^3.2.25",
    "jasmine": "^3.7.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "json-properties-remover": "^0.1.2",
    "koa-body": "^4.2.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-cleaner": "^1.0.5",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "spec-run-js": "node -r esm node_modules/jasmine/bin/jasmine.js --config=./jasmine.json",
    "spec-run": "node -r esm node_modules/jasmine/bin/jasmine.js --config=./jasmine.json --reporter=jasmine-ts-console-reporter",
    "spec": "npm-run-all spec-compile spec-run",
    "spec-compile": "npm-run-all _spec-compile-script spec-cleanup",
    "spec-compile-watch": "npm-run-all --sequential _spec-ensure-directory --parallel _spec-compile-watch-script spec-cleanup-watch",
    "compile": "npm-run-all _compile-script cleanup",
    "cleanup": "ts-cleaner --verbose --dist=dist --src=src",
    "cleanup-watch": "ts-cleaner --verbose --dist=dist --src=src --watch",
    "spec-cleanup": "ts-cleaner --verbose --dist=dist-spec --src=spec",
    "spec-cleanup-watch": "ts-cleaner --verbose --dist=dist-spec --src=spec --watch",
    "compile-watch": "npm-run-all --sequential _compile-ensure-directory --parallel _compile-script-watch cleanup-watch",
    "release": "remove-json-properties dist/package.json private",
    "postrelease": "npm publish ./dist",
    "prerelease": "npm-run-all _release-remove-compile-cache compile _release_copy-meta-files",
    "postpublish": "rimraf package.json package-lock.json LICENSE Readme.md .npmignore",
    "_compile-script": "tsc",
    "_spec-ensure-directory": "mkdirp dist-spec",
    "_compile-script-watch": "tsc -w",
    "_spec-compile-script": "tsc -p tsconfig-spec.json",
    "_spec-compile-watch-script": "tsc -w -p tsconfig-spec.json",
    "_compile-ensure-directory": "mkdirp dist",
    "_release_copy-meta-files": "cpy package.json package-lock.json LICENSE Readme.md .npmignore dist",
    "_release-remove-compile-cache": "rimraf ./dist/compiler-cache"
  },
  "private": true,
  "author": "",
  "license": "MIT"
}
